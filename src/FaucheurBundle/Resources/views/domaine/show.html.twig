{% extends '@Faucheurbase.html.twig' %}

{% block body %}
    <h1>Domaine</h1>

    <table>
        <tbody>
            <tr>
                <th>Nom</th>
                <td>{{ domaine.nom }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn waves-effect waves-light" href="{{ path('domaine_edit', { 'id': domaine.id }) }}">Modifier</a>
    <a class="btn waves-effect waves-light" href="{{ path('domaine_index') }}">Liste des domaines</a>

    {{ form_start(delete_form) }}
        <input class="btn waves-effect waves-light" type="submit" value="Delete">
    {{ form_end(delete_form) }}

{% endblock %}
