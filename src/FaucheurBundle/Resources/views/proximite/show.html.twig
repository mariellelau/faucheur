{% extends 'base.html.twig' %}

{% block body %}
    <h1>Proximite</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proximite.id }}</td>
            </tr>
            <tr>
                <th>Nomproximite</th>
                <td>{{ proximite.nomProximite }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('proximite_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('proximite_edit', { 'id': proximite.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
